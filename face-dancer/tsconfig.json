{
	"compilerOptions": {
		"target": "es2021",
		"lib": [
			"es2021"
		],
		"jsx": "react",
		"rootDir": "./src",
		"module": "nodeNext",
		"moduleResolution": "nodeNext",
		// "esModuleInterop": true,
		// "allowSyntheticDefaultImports": true,
		// "resolveJsonModule": true,
		// "allowJs": true,
		// "allowImportingTsExtensions": true,
		"noEmit": false,
		"outDir": "./target",
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
